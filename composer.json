{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "ext-json": "*",
        "aws/aws-sdk-php": "3.x",
        "colinmollenhour/credis": "~1.2",
        "daneren2005/php-resque": "dev-master#5dd9b9afd1ed866933ea621a8a85a0a2909c85d5",
        "deeplcom/deepl-php": "^1.3",
        "elasticsearch/elasticsearch": "~7.0",
        "endroid/qr-code": "5.1.0",
        "giggsey/libphonenumber-for-php": "^8.12.44",
        "league/oauth2-server": "^8.3",
        "longman/telegram-bot": "dev-master#79e5e3abc82d130aa791bd767bd0980f7b85067c",
        "michelf/php-markdown": "^2.0",
        "nickdnk/graph-sdk": "^8.0.1",
        "php-imap/php-imap": "dev-master#6b9a4647777387ecba4f8e32dd7fd31d0a6456a0",
        "slim/slim": "^4.14.0",
        "sonata-project/google-authenticator": "^3.0@dev",
        "symfony/event-dispatcher": "^7.1",
        "tgallice/fb-messenger-sdk": "dev-master#a0d103f38473891432965c0f5b38b142b5dbfc3c",
        "webklex/php-imap": "^6.2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "pint:test": [
            "./vendor/bin/pint --test"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse"
        ],
        "quality": [
            "@pint:test",
            "@analyse",
            "@test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
      {
        "type":"package",
        "package": {
          "name": "daneren2005/php-resque",
          "version":"master",
          "source": {
            "url": "https://github.com/LiveHelperChat/php-resque.git",
            "type": "git",
            "reference":"develop"
          },
          "autoload": {
            "psr-0": {
              "Resque": "lib"
            }
          }
        }
      },
      {
        "type":"package",
        "package": {
          "name": "php-imap/php-imap",
          "version":"master",
          "source": {
            "url": "https://github.com/remdex/php-imap.git",
            "type": "git",
            "reference":"master"
          },
          "autoload": {
            "psr-4": {
              "PhpImap\\": "src/PhpImap"
            }
          }
        }
      },
      {
        "type":"package",
        "package": {
          "name": "tgallice/fb-messenger-sdk",
          "version":"master",
          "source": {
            "url": "https://github.com/LiveHelperChat/fb-messenger-sdk.git",
            "type": "git",
            "reference":"master"
          },
          "autoload": {
            "psr-4": {
              "Tgallice\\FBMessenger\\": "src"
            }
          }
        }
      },
      {
        "type":"package",
        "package": {
          "name": "longman/telegram-bot",
          "version":"master",
          "source": {
            "url": "https://github.com/LiveHelperChat/telegram-core.git",
            "type": "git",
            "reference":"develop"
          },
          "autoload": {
            "psr-4": {
              "Longman\\TelegramBot\\": "src"
            }
          }
        }
      }
    ]
}
