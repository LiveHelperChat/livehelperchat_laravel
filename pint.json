{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": true,
        "nullable_type_declaration": true,
        "no_unused_imports": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "blank_line_before_statement": {
            "statements": ["return"]
        },
        "single_quote": true,
        "trailing_comma_in_multiline": true
    }
}
